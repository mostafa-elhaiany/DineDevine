    <script type="text/javascript">
      window.ezstandalone = window.ezstandalone || {};
      window.ezstandalone.cmd = window.ezstandalone.cmd || [];
      // window.ezstandalone.cmd.push(function () {
      //     window.ezstandalone.enableConsent();
      // })
      var last_path = location.href;
      var is_app = location.hash === "#in_app";
      var current_ids = [];
      window.refresh_ad = function (tag) {
        if (is_app) return;
        requestAnimationFrame(function () {
          var ad_doms = document.querySelectorAll(
            "[id^=ezoic-pub-ad-placeholder-]"
          );
          var ids = [];
          for (var i = 0, len = ad_doms.length; i < len; i += 1) {
            var id = ad_doms[i].id.match(/\d+/);
            if (!id) return;
            ids.push(+id[0]);
          }
          if (ids.length > 0) {
            window.ezstandalone.cmd.push(function () {
              if (window.ezstandalone.enabled) {
                if (location.href === last_path && tag !== "force") {
                  var moreIds = [];
                  for (var i = 0, len = ids.length; i < len; i += 1) {
                    var nid = ids[i];
                    if (current_ids.indexOf(nid) === -1) {
                      moreIds.push(nid);
                    }
                  }
                  if (moreIds.length > 0) {
                    window.ezstandalone.displayMore.apply(
                      window.ezstandalone,
                      moreIds
                    );
                    current_ids.push.apply(current_ids, moreIds);
                  }
                } else {
                  current_ids = ids;
                  window.ezstandalone.define(ids);
                  window.ezstandalone.refresh();
                  last_path = location.href;
                }
              } else {
                current_ids.push.apply(current_ids, ids);
                window.ezstandalone.define(ids);
                window.ezstandalone.enable();
                window.ezstandalone.display();
              }
            });
          }
        });
      };
      requestAnimationFrame(window.refresh_ad);
    </script>

